    # temararly
    def selected(self):
        
        choice = self.AttributesDictionary
        print(choice)
        selected = {}
        for i in choice:
            list_values = list(choice.get(i).keys())
            choice_i = list_values[random.randint(0, len(list_values) - 1)]
            selected.update({i: choice_i})
        return selected